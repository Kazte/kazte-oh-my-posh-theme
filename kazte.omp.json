{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "white": "#f7f7f7",
    "black": "#111111"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "transparent",
          "foreground": "p:white",
          "style": "plain",
          "template": "┌",
          "type": "text"
        },
        {
          "background": "#464646",
          "foreground": "p:white",
          "properties": {
            "windows": "",
            "linux": "",
            "ubuntu": "",
            "macos": ""
          },
          "style": "plain",
          "template": " {{.Icon}}{{if .WSL}} (WSL){{end}}⠀",
          "type": "os"
        },
        {
          "background": "#ffe093",
          "foreground": "p:black",
          "style": "plain",
          "template": "  {{.HostName}}{{.UserName}}⠀",
          "type": "session"
        },
        {
          "background": "#ffffff",
          "foreground": "#000000",
          "properties": {
            "fetch_stash_count": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }} {{ .HEAD }} {{ if gt .StashCount 0 }}  {{ .StashCount }} {{ end }} ",
          "type": "git"
        },
        {
          "background": "#303030",
          "foreground": "#3C873A",
          "leading_diamond": "\ue0b6",
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": " <#cc3a3a>\ue5fa</> ",
            "yarn_icon": " <#348cba>\ue6a7</>"
          },
          "style": "powerline",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "background": "#306998",
          "foreground": "#FFE873",
          "style": "powerline",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "type": "python"
        },
        {
          "background": "#0e8ac8",
          "foreground": "#ffffff",
          "style": "powerline",
          "template": " \ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "java"
        },
        {
          "background": "#0e0e0e",
          "foreground": "#0d6da8",
          "style": "powerline",
          "template": " \ue77f {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} ",
          "type": "dotnet"
        },
        {
          "background": "#ffffff",
          "foreground": "#06aad5",
          "style": "powerline",
          "template": " \ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "go"
        },
        {
          "background": "#f3f0ec",
          "foreground": "#925837",
          "style": "powerline",
          "template": " \ue7a8 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "rust"
        },
        {
          "background": "#e1e8e9",
          "foreground": "#055b9c",
          "leading_diamond": " \ue0b6",
          "style": "powerline",
          "template": " \ue798 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "dart"
        },
        {
          "background": "#ffffff",
          "foreground": "#ce092f",
          "leading_diamond": " \ue0b6",
          "style": "powerline",
          "template": " \ue753 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "angular"
        },
        {
          "background": "#1e293b",
          "foreground": "#ffffff",
          "leading_diamond": " \ue0b6",
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}Nx {{ .Full }}{{ end }} ",
          "type": "nx"
        },
        {
          "background": "#945bb3",
          "foreground": "#359a25",
          "leading_diamond": " \ue0b6",
          "style": "powerline",
          "template": " <#ca3c34>\ue624</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "julia"
        },
        {
          "background": "#ffffff",
          "foreground": "#9c1006",
          "style": "powerline",
          "template": " \ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "ruby"
        },
        {
          "background": "#ffffff",
          "foreground": "#5398c2",
          "style": "powerline",
          "template": " \uf104<#f5bf45>\uf0e7</>\uf105 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "azfunc"
        },
        {
          "background": "#565656",
          "foreground": "#faa029",
          "style": "powerline",
          "template": " \ue7ad {{.Profile}}{{if .Region}}@{{.Region}}{{end}} ",
          "type": "aws"
        },
        {
          "background": "#316ce4",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b6",
          "style": "powerline",
          "template": " \uf308 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}} ",
          "trailing_diamond": "\ue0b4",
          "type": "kubectl"
        },
        {
          "type": "sysinfo",
          "foreground": "#ffffff",
          "background": "#8f43f3",
          "template": "  {{ round .PhysicalPercentUsed .Precision }}% ",
          "properties": {
            "precision": 2
          },
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "background": "transparent",
          "foreground": "#b3ffde",
          "properties": {
            "time_format": "02/01/06 15:04"
          },
          "style": "plain",
          "template": "<#93d0ff>[</>{{.CurrentDate | date .Format}}",
          "type": "time"
        },
        {
          "background": "transparent",
          "foreground": "p:white",
          "style": "plain",
          "template": "<#93d0ff>]</>┐",
          "type": "text"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "transparent",
          "foreground": "p:white",
          "properties": {
            "folder_icon": "",
            "folder_separator_template": "<#93d0ff> » </>",
            "home_icon": "",
            "style": "agnoster"
          },
          "style": "plain",
          "template": "<p:white>└</><#93d0ff>[</> {{.Path}} <#93d0ff>]</>",
          "type": "path"
        },
        {
          "type": "spotify",
          "style": "powerline",
          "powerline_symbol": "",
          "background": "transparent",
          "foreground": "#1BD760",
          "properties": {
            "playing_icon": " ",
            "paused_icon": " ",
            "stopped_icon": " "
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "background": "transparent",
          "foreground": "#b3ffde",
          "properties": {
            "style": "austin",
            "threshold": 0
          },
          "style": "plain",
          "template": "<#93d0ff>[</>{{.FormattedMs}}<#93d0ff>]</>",
          "type": "executiontime"
        },
        {
          "background": "transparent",
          "foreground": "p:white",
          "style": "plain",
          "template": "┘",
          "type": "text"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "transparent",
          "foreground": "#b3ffde",
          "foreground_templates": ["{{if gt .Code 0}}#ff3030{{end}}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": ">",
          "type": "status"
        }
      ]
    }
  ],
  "console_title_template": "{{.Folder}}",
  "final_space": true,
  "version": 2,
  "Transform": "AWS::Serverless-2016-10-31"
}
